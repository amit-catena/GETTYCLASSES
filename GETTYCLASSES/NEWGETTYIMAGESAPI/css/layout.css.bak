*{margin: 0; padding: 0; box-sizing: border-box;}
body{margin:0;padding:0;  font: 18px/24px arial;background: url(../images/main-bg-overlay.png) repeat-x left top, url(../images/main-bg.jpg) repeat left top;}

    .midwrp .innersec>li {display: inline-block;width: 20%;box-sizing: border-box;padding: 10px;vertical-align: top;margin: 0; position:relative;cursor:pointer;}

.midwrp .innersec li .inrimg {min-width: 180px; display: block;vertical-align: top;margin: auto;text-align: center;}
.midwrp{ margin: auto; padding: 20px; }
.midwrp .innersec{margin: 0; padding: 0;list-style: none;}
.midwrp .innersec>li{padding: 5px;}




.midwrp .innersec>li .mainlidiv {border: 1px solid #ccc;padding: 10px; border-radius: 5px;}
.midwrp .innersec li .inrimg img{ margin-right: 15px; max-width:100%; }
.midwrp .innersec li .downdetail{ width: 100%;display: table-cell;vertical-align: top; }
.midwrp .innersec li .downdetail .dow-date,.midwrp .innersec>li .downdetail>ul{ display: block; }
.downdetail .dow-date em{ display: block;font-style: normal; font-size: 13px; }
.downdetail .dow-date{padding-bottom: 15px; border-bottom: 1px solid #bfbfbf; position: relative;}
.downdetail .dow-date span{ font-size: 18px;}
.downdetail .dow-date span:nth-child(3){position: absolute; right: 0;}
.downdetail ul {margin-top:8px; padding: 0; list-style: none;}
.downdetail ul li span{ display: inline-block; font-size: 14px;vertical-align: top; }
.downdetail ul li:nth-child(2) span:nth-child(2) {height: 50px;width:calc(100% - 58px);overflow: hidden;text-overflow: ellipsis;}
.downdetail ul li span a{ text-decoration: none; color: #03A9F4; }
.downdetail ul li { margin-bottom: 8px; }
.downdetail ul li:nth-child(3) span{max-height:50px; overflow:hidden;}
.downdetail ul li:nth-child(2) span:nth-child(2) input{display:inline-block; width:18px; height:18px; vertical-align:middle; margin-right:5px; }
.downdetail ul li span:nth-child(1){width: 47px;color: #8f8f8f;}
.serchtxt {width: 800px; margin: auto;}
.serchtxt span:nth-child(1){text-transform: uppercase;font-size: 14px;}
.serchtxt span input[type=text], select {padding: 9px 20px;margin: 8px 0;display: inline-block;border: 1px solid #ccc;border-radius: 2px;box-sizing: border-box;
                                         margin-left: 10px;width: 300px;}
.serchtxt span input[type=button] {background-color: #284da1;color: white;padding: 10px 20px;margin: 8px 0;border: none;border-radius: 2px;cursor: pointer;}
.serchtxt span input[type=button]:hover {background-color: #325dbf;}
.loader{display:none;position: absolute;top: 50%;left: 50%;transform: translate(-50%, -50%);}
.midwrp span input[type=button] {    background-color: #284da1;    color: white;    padding: 10px 20px;    margin: 8px 0;    border: none;    border-radius: 2px;    cursor: pointer;
}
.sernew {margin-top:20px;
}

.loadermore{display:none;position: absolute;bottom:0;left: 50%;transform: translateX(-50%);}
.box {
     display:none;
     position: absolute;
       
    background: #FFF;
    padding: 5px;
    border: 1px solid #ccc;
    width: 768px;
    z-index: 999;
	border-radius:5px; font-size:14px; 
}

a:hover + .box {display:block;}
.inrimg:hover .box{ display:block; }
span.innerinrimg {float: left; max-width: 250px; margin-right: 10px;}
.innerpopup { vertical-align: top; padding: 5px; box-sizing: border-box;  text-align: left;}
.innerpopup span:first-child{display:block; font-weight:bold; font-size:15px; padding-bottom:5px;margin-bottom:5px; border-bottom:1px solid #ccc;  }


